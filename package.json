{
    "name": "entro-ci",
    "description": "An automated CI tool to help with building packages and ignoring them when there wasn't a change.",
    "version": "1.5.0",
    "author": "Kerren @Kerren-Entrostat",
    "bin": {
        "entro-ci": "./bin/run"
    },
    "bugs": "https://github.com/entrostat/entro-ci/issues",
    "dependencies": {
        "@oclif/command": "^1.8.0",
        "@oclif/config": "^1.17.0",
        "@oclif/plugin-help": "^3.2.0",
        "class-transformer": "^0.3.1",
        "class-validator": "^0.12.2",
        "cli-ux": "^5.5.0",
        "fs-extra": "^9.0.1",
        "got": "^11.8.1",
        "tslib": "^1.14.1"
    },
    "devDependencies": {
        "@oclif/dev-cli": "^1.22.2",
        "@oclif/test": "^1.2.7",
        "@types/chai": "^4.2.14",
        "@types/fs-extra": "^8.1.1",
        "@types/mocha": "^5.2.7",
        "@types/node": "^10.17.50",
        "chai": "^4.2.0",
        "eslint": "^5.16.0",
        "eslint-config-oclif": "^3.1.0",
        "eslint-config-oclif-typescript": "^0.1.0",
        "globby": "^10.0.2",
        "mocha": "^5.2.0",
        "nyc": "^14.1.1",
        "prettier": "^2.1.2",
        "standard-version": "^9.0.0",
        "ts-node": "^8.10.2",
        "typescript": "^3.9.7"
    },
    "engines": {
        "node": ">=8.0.0"
    },
    "files": [
        "/bin",
        "/lib",
        "/npm-shrinkwrap.json",
        "/oclif.manifest.json"
    ],
    "homepage": "https://github.com/entrostat/entro-ci",
    "keywords": [
        "oclif"
    ],
    "license": "MIT",
    "main": "lib/index.js",
    "oclif": {
        "commands": "./lib/commands",
        "bin": "entro-ci",
        "plugins": [
            "@oclif/plugin-help"
        ]
    },
    "repository": "entrostat/entro-ci",
    "scripts": {
        "postpack": "rm -f oclif.manifest.json",
        "posttest": "eslint . --ext .ts --config .eslintrc",
        "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
        "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
        "version": "oclif-dev readme && git add README.md",
        "release": "standard-version",
        "release:dry-run": "standard-version --dry-run"
    },
    "types": "lib/index.d.ts"
}
